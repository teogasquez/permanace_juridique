generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Booking {
  id        String   @id @default(cuid())
  date      DateTime
  time      String
  name      String
  email     String
  phone     String
  subject   String
  createdAt DateTime @default(now())
}

model Admin {
  id       String @id @default(cuid())
  email    String @unique
  password String
}